{
  "name": "aidumpster",
  "displayName": "aidumpster",
  "description": "PLACEHOLDER",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:aidumpster.showUI"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "codebase-ai-view-container",
          "title": "Codebase AI",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "codebase-ai-view-container": [
        {
          "id": "codebase-ai-view",
          "name": "Codebase AI",
          "type": "webview"
        }
      ]
    },
    "commands": [
      {
        "command": "aidumpster.showUI",
        "title": "Open Codebase AI"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
